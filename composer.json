{
    "name": "eclipxe/engineworks-templates",
    "description": "PHP Templates with plugins",
    "homepage": "https://github.com/eclipxe13/engineworks-templates",
    "license": "MIT",
    "keywords": ["native templates", "php templates", "templates"],
    "authors": [
        {
            "name": "Carlos C Soto",
            "email": "eclipxe13@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "suggest": {
        "slim/slim": "^3.8",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "slim/slim": "^3.8",
        "psr/http-message": "^1.0",
        "phpunit/phpunit": "^6.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "EngineWorks\\Templates\\": "src/EngineWorks/Templates/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\EngineWorks\\Templates\\": "tests/EngineWorks/Templates/"
        }
    }
}
