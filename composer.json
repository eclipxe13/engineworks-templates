{
    "name": "eclipxe/engineworks-templates",
    "description": "PHP Templates with plugins",
    "homepage": "https://github.com/eclipxe13/engineworks-templates",
    "license": "MIT",
    "keywords": ["native templates", "php templates", "templates"],
    "authors": [
        {
            "name": "Carlos C Soto",
            "email": "eclipxe13@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "slim/php-view": "@stable"
    },
    "suggest": {
        "slim/slim": "@stable",
        "psr/http-message": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "jakub-onderka/php-parallel-lint": "@stable",
        "slim/slim": "@stable",
        "psr/http-message": "@stable",
        "scrutinizer/ocular": "@stable",
        "satooshi/php-coveralls": "@stable"
    },
    "autoload": {
        "psr-4": {
            "EngineWorks\\Templates\\": "sources/EngineWorks/Templates/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\EngineWorks\\Templates\\": "tests/EngineWorks/Templates/"
        }
    }
}
